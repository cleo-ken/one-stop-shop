{
  "name": "one-stop-shop",
  "version": "1.0.0",
  "private": true,
  "description": "One-Stop-Shop internal content tool prototype",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace server\" \"npm run dev --workspace client\"",
    "build": "npm run build --workspace client",
    "start": "npm run start --workspace server"
  },
  "devDependencies": {
    "concurrently": "^9.0.1"
  }
}
